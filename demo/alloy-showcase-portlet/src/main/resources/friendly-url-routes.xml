<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.1.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_1_0.dtd">

<routes>
	<route>
		<pattern>/{componentPrefix}/{componentName}/{componentUseCase}/foo/{foo}</pattern>
		<ignored-parameter name="_facesViewIdRender" />
		<ignored-parameter name="countryId" />
		<ignored-parameter name="menuItem" />
		<overridden-parameter name="_facesViewIdRender">/views/component.xhtml</overridden-parameter>
		<overridden-parameter name="countryId"></overridden-parameter>
		<overridden-parameter name="menuItem"></overridden-parameter>
	</route>
	<route>
		<pattern>/{componentPrefix}/{componentName}/{componentUseCase}/countryId/{countryId}</pattern>
		<ignored-parameter name="_facesViewIdRender" />
		<ignored-parameter name="foo" />
		<ignored-parameter name="menuItem" />
		<overridden-parameter name="_facesViewIdRender">/views/component.xhtml</overridden-parameter>
		<overridden-parameter name="menuItem"></overridden-parameter>
	</route>
	<route>
		<pattern>/{componentPrefix}/{componentName}/{componentUseCase}/{menuItem}</pattern>
		<ignored-parameter name="_facesViewIdRender" />
		<ignored-parameter name="countryId" />
		<overridden-parameter name="_facesViewIdRender">/views/component.xhtml</overridden-parameter>
		<overridden-parameter name="countryId"></overridden-parameter>
	</route>
	<route>
		<pattern>/{componentPrefix}/{componentName}/{componentUseCase}</pattern>
		<ignored-parameter name="_facesViewIdRender" />
		<ignored-parameter name="countryId" />
		<ignored-parameter name="menuItem" />
		<overridden-parameter name="_facesViewIdRender">/views/component.xhtml</overridden-parameter>
		<overridden-parameter name="countryId"></overridden-parameter>
		<overridden-parameter name="menuItem"></overridden-parameter>
	</route>
</routes>
